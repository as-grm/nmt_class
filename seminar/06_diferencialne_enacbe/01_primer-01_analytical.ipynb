   "id": "2725ba4f-ff62-491e-a20e-642bf12cdc72",
   "metadata": {},
   "outputs": [],
   "source": [
    "de = diff(y,x) == y - x^2 + 1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "67a03c74-ff4d-4b76-9ae9-76b1c58150f7",
   "metadata": {},
   "outputs": [],
   "source": [
    "f = desolve(de,y,ics=[0,0.5])"
   ]
  },
